config_version=5

[application]

config/name="Factory_Contractor"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://art/game_icons/factory-purple.svg"

[autoload]

MessageBus="*res://scripts/autoloads/MessageBus.gd"
Contracts="*res://scripts/autoloads/Contracts.gd"
ComponentDB="*res://scripts/autoloads/ComponentDB.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=480
window/stretch/mode="canvas_items"

[file_customization]

